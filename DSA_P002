#include <bits/stdc++.h>
using namespace std;
typedef long long ll;

int n, a[20] = {0};
int c = 1;

void in() {
    cout << c << ": ";
    c++;
	for (int i = 1; i <= n; i++) {
		cout << a[i] << " ";
	}
	cout << "\n";
}

void init() {
	for (int i = 1; i <= n; i++) {
		a[i] = i;
	}
}

void sinhhvi() {
	in();
	int i = n-1;
	while (i > 0) {
		if (a[i] < a[i+1]) break;
		i--;
	}
	if (i == 0) return;
	int j, k = n;
	while (a[k] < a[i]) k--;
	swap(a[i], a[k]);
	j = i + 1; k = n;
	while (j < k) {
		swap(a[j], a[k]);
		j++; k--;
	}
	sinhhvi();
	
}

int main(){

    ios_base::sync_with_stdio(0);
    cin.tie(0);
    cout.tie(0);

    cin >> n;
    init();
    sinhhvi();
}
