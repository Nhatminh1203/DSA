#include <bits/stdc++.h>
using namespace std;
typedef long long ll;


int n, k;
int a[21], visited[21];

void backtrack(int pos){
    if(pos > n){
        for(int i = 1; i <= n; i++){
            cout << a[i];
        }
        cout << " ";
        return;
    }
    for(int i = n; i >= 1; i--){
        if(visited[i] == 0){
            a[pos] = i;
            visited[i] = 1;
            backtrack(pos + 1);
            visited[i] = 0;
        }
    }
}

int main(){

    ios_base::sync_with_stdio(0);
    cin.tie(0);
    cout.tie(0);

    memset(visited, 0, sizeof(visited));
    int t;
    cin >> t;
    while(t--){
        cin >> n;
        backtrack(1);
        cout << "\n";
    }
}
