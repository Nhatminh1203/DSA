#include <bits/stdc++.h>
using namespace std;
typedef long long ll;

int n;
char a[21];
char s[2] = {'A', 'B'};

void Try(int pos){
    if(pos > n){
        for(int i = 1; i <= n; i++){
            cout << a[i];
        }
        cout << " ";
        return;
    }

    for(int i = 0; i <= 1; i++){
        a[pos] = s[i];
        Try(pos + 1);
    }
}

int main(){

    ios_base::sync_with_stdio(0);
    cin.tie(0);
    cout.tie(0);

    int t;
    cin >> t;
    while(t--){
        cin >> n;
        Try(1);
        cout << "\n";
    }

}
