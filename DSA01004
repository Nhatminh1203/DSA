#include <bits/stdc++.h>
using namespace std;
typedef long long ll;


int n, k;
int a[21], visited[21];

void backtrack(int pos){
    if(pos > k){
        for(int i = 1; i <= k; i++){
            cout << a[i];
        }
        cout << " ";
        return;
    }
    for(int i = a[pos - 1] + 1; i <= n - k + pos; i++){
        a[pos] = i;
        backtrack(pos + 1);
    }
}

int main(){

    ios_base::sync_with_stdio(0);
    cin.tie(0);
    cout.tie(0);

    memset(visited, 0, sizeof(visited));
    int t;
    cin >> t;
    while(t--){
        cin >> n >> k;
        backtrack(1);
        cout << "\n";
    }
}
