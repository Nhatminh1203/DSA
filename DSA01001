#include <bits/stdc++.h>
using namespace std;
typedef long long ll;

string s;
void sinhnpkt(){
        int i = s.length()-1;
        while(i >= 0 && s[i] == '1'){
            s[i] = '0';
            i--;
        }
        if(i != -1){
            s[i] = '1';
        }
        cout << s << "\n"; 
}

int main(){

    ios_base::sync_with_stdio(0);
    cin.tie(0);
    cout.tie(0);

    int t;
    cin >> t;
    while(t--){
        cin >> s;
        sinhnpkt();
    }
}
