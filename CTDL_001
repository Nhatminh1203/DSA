#include <bits/stdc++.h>
using namespace std;
typedef long long ll;

int n, a[20] = {0};

int check(){
    for(int i = 1; i <= n/2; i++){
        if(a[i] != a[n - i + 1]) return 0;
    }
    return 1;
}

void in(){
        for(int i = 1; i <= n; i++){
        cout << a[i] << " ";
    }
    cout << "\n";
}
void sinhnp(){
    while(1){
        if(check()) in();
        int i = n;
        while(a[i] == 1){
            a[i] = 0;
            i--;
        }
        if(i==0) break;
        else(a[i] = 1);
    }
}

int main(){

    ios_base::sync_with_stdio(0);
    cin.tie(0);
    cout.tie(0);

    cin >> n;
    sinhnp();
}
